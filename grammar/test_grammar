

asdf = 'qert'
qwert = /testing/msix

tokens {}

context root {
	'context', 'root' {
		return
	}
	'asdf' {
		spawn undef
		spawn $0
		spawn 'asdf'
		spawn !asdf
		spawn [ !asdf ]
	}
	'qwerty', /\d+/i {
		assign {
			'asdf' => 'qwerty'
			'qwerty'[] => 'zxcv'
			'zxvc'{'key'} => 'uiop'
		}
		spawn []
		spawn {}
	}
	default {
		warn 'default path'
		return
	}
}

context test_more {
	'a' {
		match 'b', 'c', $qwert
	}
}
