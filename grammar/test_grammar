

asdf = 'qert'
qwert = /testing/msix

tokens {}

object context root {
	'context', 'root' {
		return
	}
	'asdf' {
		spawn undef
		spawn $0
		spawn 'asdf'
		spawn !test_more
		spawn [ !test_more ]
	}
	'qwerty', /\d+/i {
		assign {
			'asdf' => 'qwerty'
			'qwerty'[] => 'zxcv'
			'zxvc'{'key'} => 'uiop'
		}
		spawn []
		spawn {}
	}
	default {
		warn 'default path'
		return
	}
}

context test_more {
	'a' {
		match 'b', 'c', $qwert
	}
	'b' {
		spawn {
			'a' => '15'
			'b' => $0
			'nested_hash' => {
				'nested' => 'true'
				'c' => '35'
			}
		}
	}
}
